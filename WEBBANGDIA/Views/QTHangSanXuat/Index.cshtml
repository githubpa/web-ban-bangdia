@model IEnumerable<WEBBANGDIA.Models.HangSanXuat>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.MaHSX)*@
            Mã hàng sản xuất
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.TenHSX)*@
            Tên hàng sản xuất
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.QuocGia.TenQG)*@
            Tên quốc gia
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.NgayTao)*@
            Ngày tạo
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.An)*@
            Ẩn
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.NgayAn)*@
            Ngày ẩn
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaHSX)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenHSX)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.QuocGia.TenQG)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NgayTao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.An)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NgayAn)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.MaHSX }) |
            @Html.ActionLink("Details", "Details", new { id = item.MaHSX }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.MaHSX })
        </td>
    </tr>
}

</table>
