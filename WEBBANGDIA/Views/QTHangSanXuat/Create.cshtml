@model WEBBANGDIA.Models.HangSanXuat

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>HangSanXuat</legend>

        <div class="editor-label">
            Mã hàng sản xuất
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.MaHSX)
            @Html.ValidationMessageFor(model => model.MaHSX)
        </div>

        <div class="editor-label">
            Tên hàng sản xuất
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.TenHSX)
            @Html.ValidationMessageFor(model => model.TenHSX)
        </div>

        <div class="editor-label">
            Quốc gia
        </div>
        <div class="editor-field">
            @Html.DropDownList("MaQG", String.Empty)
            @Html.ValidationMessageFor(model => model.MaQG)
        </div>

        <div class="editor-label">
           Ngày tạo
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.NgayTao)
            @Html.ValidationMessageFor(model => model.NgayTao)
        </div>

        <div class="editor-label">
            Ẩn
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.An)
            @Html.ValidationMessageFor(model => model.An)
        </div>

        <div class="editor-label">
            Ngày ẩn
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.NgayAn)
            @Html.ValidationMessageFor(model => model.NgayAn)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
